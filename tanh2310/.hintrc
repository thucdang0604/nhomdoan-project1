{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "highest-available-document-mode": "off",
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-tap-highlight-color"
        ]
      }
    ],
    "detect-css-reflows/layout": "off",
    "axe/structure": [
      "default",
      {
        "listitem": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 109",
    "not firefox <= 110",
    "not safari <= 16.3"
  ]
}